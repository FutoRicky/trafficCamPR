{{#md-modal close="close"}}
  <div class="card">
    <div class="card-image">
      <img src="{{model.imageSource}}">
    </div>
    <div class="card-content">
      <span class="card-title grey-text text-darken-4">
        {{model.name}}
      </span>
      <p>{{model.location}}</p>
    </div>
  </div>
{{/md-modal}}